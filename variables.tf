variable "company_name" {
  default = "keytwine"
}

variable "aws_default_region" {
    default = "eu-west-1"
}

variable "root_account_id" {
    description = "Root account id"
}

variable "sub_account_id_sandbox" {
    description = "AWS Sub account id for sandbox account"
}

variable "sub_account_id_gfl" {
    description = "AWS Sub account id for gfl account"
}

# gpg --export -a 2EA619ED
variable "hector_pgp_public_key" {
    default = ""
}

# gpg --export B016853D | base64
variable "gfl_pgp_public_key" {
    default = ""
}

variable "allowed_ips" {
  description = "List of IPs allowed to operate with these accounts"
  type        = "list"
}
